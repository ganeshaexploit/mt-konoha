-- ‚ö° Brutal Teleport Multi-Point by Ganesha Exploit ‚ö°
-- ‚úÖ Brutal CFrame (instan + anti mantul)
-- ‚úÖ Delay 60s antar checkpoint
-- ‚úÖ UI Modern ON/OFF + Hotkey [T]
-- ‚úÖ Auto lanjut checkpoint terakhir saat respawn
-- ‚úÖ Auto reset 1s setelah checkpoint terakhir (loop ulang)
-- ‚úÖ UI OFF = script otomatis berhenti
-- ‚úÖ Notifikasi asli Roblox (Auto Summit by Ganesha Exploit)

--=== Services ===--
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local UIS = game:GetService("UserInputService")
local player = Players.LocalPlayer

--=== Checkpoints (CFrame) ===--
local checkpoints = {
    CFrame.new(813.32,285.00,-576.73),
    CFrame.new(770.99,516.98,-379.52),
    CFrame.new(-79.29,473.00,386.61),
    CFrame.new(179.59,581.00,701.10),
    CFrame.new(348.56,584.72,820.30),
    CFrame.new(793.17,809.00,625.78),
    CFrame.new(915.12,997.73,584.24),
}

--=== Settings ===--
local teleporting = false
local delayTime = 60 -- ‚è±Ô∏è delay antar checkpoint (60 detik)
local lastCheckpoint = 1

--=== Notifikasi Roblox ===--
local function Notify(msg)
    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = "‚ö° Auto Summit by Ganesha Exploit",
            Text = msg,
            Duration = 3
        })
    end)
end

--=== Loop Teleport Brutal ===--
local function teleportLoop()
    while teleporting do
        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")
        local hum = char:WaitForChild("Humanoid")

        for i = lastCheckpoint, #checkpoints do
            if not teleporting then break end
            hrp.CFrame = checkpoints[i]
            lastCheckpoint = i
            if i < #checkpoints then
                task.wait(delayTime)
            end
        end

        -- selesai semua checkpoint
        if teleporting and lastCheckpoint == #checkpoints then
            Notify("Auto Reset dalam 1s...")
            task.wait(1)
            if teleporting then
                hum.Health = 0 -- reset natural
                lastCheckpoint = 1
                break
            end
        end
    end
end

--=== Auto lanjut saat respawn ===--
player.CharacterAdded:Connect(function()
    task.wait(1)
    if teleporting then
        teleportLoop()
    end
end)

--=== UI Modern ===--
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0,200,0,80)
Frame.Position = UDim2.new(0.05,0,0.2,0)
Frame.BackgroundColor3 = Color3.fromRGB(25,25,25)
Frame.BackgroundTransparency = 0.2
Frame.BorderSizePixel = 0
Instance.new("UICorner", Frame).CornerRadius = UDim.new(0,12)

local ToggleBtn = Instance.new("TextButton", Frame)
ToggleBtn.Size = UDim2.new(0,180,0,40)
ToggleBtn.Position = UDim2.new(0,10,0,20)
ToggleBtn.Text = "üî¥ Teleport [OFF]"
ToggleBtn.BackgroundColor3 = Color3.fromRGB(40,40,40)
ToggleBtn.TextColor3 = Color3.fromRGB(255,255,255)
ToggleBtn.Font = Enum.Font.GothamBold
ToggleBtn.TextSize = 14
Instance.new("UICorner", ToggleBtn).CornerRadius = UDim.new(0,10)

--=== Toggle Function ===--
ToggleBtn.MouseButton1Click:Connect(function()
    if not teleporting then
        teleporting = true
        ToggleBtn.Text = "üü¢ Teleport [ON]"
        Notify("Teleport Dimulai")
        task.spawn(teleportLoop)
    else
        teleporting = false
        ToggleBtn.Text = "üî¥ Teleport [OFF]"
        Notify("Teleport Berhenti")
    end
end)

--=== Hotkey [T] ===--
UIS.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.T then
        ToggleBtn:MouseButton1Click()
    end
end)

--=== Stop Teleport kalau UI dihapus ===--
ScreenGui:GetPropertyChangedSignal("Parent"):Connect(function()
    if not ScreenGui.Parent then
        teleporting = false
        Notify("UI dimatikan, teleport berhenti.")
    end
end)

Notify("‚úÖ Script Loaded! Tekan tombol [T] untuk toggle.")
